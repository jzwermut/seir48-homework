<h1>
  <%= @song.name %>
  <% if @song.thumbs_up == 1 %>
    <span>| 👍</span>
  <% end %>
</h1>

<% unless @song.image.nil? || @song.image.empty? %>
  <iframe src="<%= @song.image %>" width="300" height="380" frameborder="0" allowtransparency="true" allow="encrypted-media"></iframe>
<% end %>

<div class="controls">
  <a href="/songs/<%= @song.id %>/edit">Edit Song</a>
  <a href="/songs/<%= @song.id %>/delete">Delete Song</a>
</div>
