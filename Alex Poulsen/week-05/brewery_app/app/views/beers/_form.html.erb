
<%= form_for @beer do |f| %>

<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :abv %>
<%= f.number_field :abv %>

<%= f.label :image %>
<%= f.url_field :image %>

<%= f.label :beer_style %>
<%= f.text_field :beer_style %>

<%= f.label :brewery_id %>
<%= f.select :brewery_id, Brewery.pluck(:name, :id), :include_blank => true %>

<%= f.submit %>
<% end %>
